{
	"name": "cpap_calculator",
	"version": "1.0.0",
	"description": "compare different effective pressure models for positive airway pressure devices, built using the MERN stack. ",
	"main": "server.js",
	"scripts": {
		"client-install": "cd client && npm install",
		"client-build": "cd client && npm run build",
		"start": "node server.js",
		"server": "nodemon server.js",
		"client": "npm start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cantorsdustbunnies/cpap_calculator.git"
	},
	"keywords": [
		"node",
		"express",
		"mongoDB",
		"mongoose",
		"cpap",
		"react"
	],
	"author": "Kelly Stratton",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/cantorsdustbunnies/cpap_calculator/issues"
	},
	"homepage": "https://github.com/cantorsdustbunnies/cpap_calculator#readme",
	"dependencies": {
		"@hapi/joi": "^15.0.3",
		"body-parser": "^1.20.1",
		"dotenv": "^8.0.0",
		"express": "^4.18.2",
		"mongoose": "^5.5.14"
	}
}
